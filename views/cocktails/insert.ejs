<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
  </head>
  <body>
    <%- include('../partials/nav') %>

    <div class="container mb-3">
      <h3 class="ml-5 mt-5">테마에 칵테일 추가</h3>
      <div class="mt-3 ml-5">
        <form action="/cocktails/insert/result" method="get">
          <select name="thema">
            <% themas.forEach(function(thema) { %>
            <option value="<%= thema._id %>"><%= thema.name %></option>
            <% }) %>
          </select>
          <select name="cocktail" multiple>
            <% for(var i = 0; i < cocktails.length; i++) { %>
            <option value="<%= cocktails[i].name %>"><%= cocktails[i].name %></option>
            <% } %>
          </select>
          <!-- <select name="thema"></select> -->
          <button type="submit">button</button>
        </form>
      </div>
    </div>
  </body>
</html>
